    <body>
      <div type="edition" xml:id="edition-text" xml:space="preserve">
        <div type="textpart" n="1" xml:id="part1">
           <head>Book 1</head>
          
            <div type="textpart" subtype="Poem" n="1" xml:id="book1poem1">
          <head>Poem 1</head>
          <lg met="elegiaiccouplets">
                            <lg type="couplet">
                                <l n="1" xml:id="l1.1">
                                    <persName ref="#Cynthia">Cynthia</persName>
                                    <app>
                                        <lem wit="#A #N #P #V #Vo">prima</lem>
                                        <rdg wit="#P">sola</rdg>
                                    </app> suis miserum me cepit ocellis,</l>
                                <l n="2" xml:id="l1.2">contactum nullis ante cupidinibus</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="3" xml:id="l1.3">
                                    <app>
                                        <lem wit="#N #V #Vo">tum</lem>
                                        <rdg wit="#A #F #P">tunc</rdg>
                                    </app> mihi constantis deiecit lumina fastus</l>
                                <l n="4" xml:id="l1.4">et caput impositis pressit Amor pedibus,</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="5" xml:id="l1.5">donec me <app>
                                        <lem wit="#A #N #P #Vo">docuit castas</lem>
                                        <rdg wit="#V">
                                            <choice>
                                                <sic>castas docuit</sic>
                                                <corr type="V2">docuit castas</corr>
                                            </choice>
                                        </rdg>
                                    </app> odisse puellas</l>
                                <l n="6" xml:id="l1.6">improbus, et nullo vivere consilio.</l>
                            </lg>
                       
                            <lg type="couplet">
                                <l n="7" xml:id="l1.7">ei mihi, iam <app>
                                        <lem wit="#N">toto furor hic non deficit anno,</lem>
                                        <rdg wit="#P #V #Vo">totis furor hic non deficit
                                            annis,</rdg>
                                        <rdg wit="#A #F">toto furor hic non deficit annis,</rdg>
                                    </app>
                                </l>
                                <l n="8" xml:id="l1.8">cum tamen adversos cogor habere deos.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="9" xml:id="l1.9">
                                    <app>
                                        <lem>
                                            <persName ref="#Milanion">Milanion</persName>
                                        </lem>
                                        <rdg wit="#N #A #F #P #V">minalion</rdg>
                                        <rdg wit="#F">mimalion</rdg>
                                    </app>
                                    <app>
                                        <lem>nullos</lem>
                                        <rdg wit="#A #F">nullo</rdg>
                                    </app> fugiendo, <persName ref="#Tullus">Tulle</persName>,
                                    labores</l>
                                <l n="10" xml:id="l1.10">saevitiam durae contudit <persName ref="#Iasus"
                                        >Iasidos</persName>.</l>
                            </lg>
                        
                          <lg type="couplet">
                            <l n="11" xml:id="l1.11">nam modo Partheniis amens errabat in antris,</l>
                            <l n="12" xml:id="l1.12">ibat et hirsutas ille videre feras</l>
                          </lg>
                        
                <lg type="couplet">
                            <l n="13" xml:id="l1.13">ille etiam Hylaei percussus vulnere rami</l>
                            <l n="14" xml:id="l1.14">saucius Arcadiis rupibus ingemuit.</l>
                </lg>
              
                <lg type="couplet">
                            <l n="15" xml:id="l1.15">ergo velocem potuit domuisse puellam:</l>
                            <l n="16" xml:id="l1.16">tantum in amore preces et bene facta valent.</l>
                </lg>
              
                            <lg type="couplet">
                                <l n="17" xml:id="l1.17">in me tardus Amor non [n]ullas cogitat artes,</l>
                                <l n="18" xml:id="l1.18">nec <app>
                                        <lem>meminit</lem>
                                        <rdg wit="#A #F #P"/>
                                    </app> notas, ut prius, ire vias.</l>
                            </lg>
                            <lg type="couplet">
                                <l n="19" xml:id="l1.19">
                                    <app>
                                        <lem>at</lem>
                                        <rdg source="#Hartman">aut</rdg>
                                    </app> vos,<app><lem>deductae</lem><rdg wit="#A #F">deducto</rdg></app> quibus est <app>
                                        <lem>fallacia</lem>
                                        <rdg source="#Fruter">pellacia</rdg>
                                    </app>lunae</l>
                                <l n="20" xml:id="l1.20">et labor in magicis <app>
                                        <lem>sacra</lem>
                                        <rdg source="#Fontein">fata</rdg>
                                        <rdg source="#Unger">astra</rdg>
                                    </app> piare focis,</l>
                            </lg>
                            <lg type="couplet">
                                <l n="21" xml:id="l1.21">en agedum dominae mentem convertite nostrae,</l>
                                <l n="22" xml:id="l1.22">et facite illa meo <app>
                                        <lem>palleat</lem>
                                        <rdg wit="#A #F">placeat</rdg>
                                    </app> ore magis!</l>
                            </lg>
                       
               
                            <lg type="couplet">
                                <l n="23" xml:id="l1.23">tunc ego crediderim Manes et sidera vobis</l>
                                <l n="24" xml:id="l1.24">posse <app>
                                        <lem>
                                            <sic>cythalinis</sic>
                                        </lem>
                                        <rdg wit="#F #P">citalinis</rdg>
                                        <rdg wit="#A">cithalinis</rdg>
                                        <rdg wit="#V #Vo">cytalinis</rdg>
                                    </app> ducere carminibus.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="25" xml:id="l1.25">
                                    <app>
                                        <lem source="#Hemsterhuys #Leo">aut</lem>
                                        <rdg wit="#N #A #P #V #Vo">et</rdg>
                                        <rdg wit="#F">
                                            <choice>
                                                <sic>et</sic>
                                                <corr type="F4">at</corr>
                                            </choice>
                                        </rdg>
                                    </app> vos, qui sero lapsum revocatis, amici,</l>
                                <l n="26" xml:id="l1.26">quaerite non sani pectoris auxilia.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="27" xml:id="l1.27">fortiter et ferrum saevos patiemur et ignis,</l>
                                <l n="28" xml:id="l1.28">sit modo libertas quae velit ira loqui.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="29" xml:id="l1.29">
                                    <app>
                                        <lem wit="#N #P #V #Vo">ferte</lem>
                                        <rdg wit="#A">forte</rdg>
                                        <rdg wit="#F">
                                            <choice>
                                                <sic>forte</sic>
                                                <corr type="F3">fer(r)e</corr>
                                            </choice>
                                        </rdg>
                                    </app> per extremas gentes et <app>
                                        <lem wit="#N #P #V #Vo">ferte</lem>
                                        <rdg wit="#A #F">fer(r)e</rdg>
                                    </app> per undas,</l>
                                <l n="30" xml:id="l1.30">qua non ulla meum femina norit iter:</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="31" xml:id="l1.31">vos remanete, quibus <app>
                                        <lem wit="#N #A #P #V #Vo">facili</lem>
                                        <rdg wit="#F">facilis</rdg>
                                    </app> deus annuit <app>
                                        <lem wit="#N #A #F #P #V #Vo">aure</lem>
                                        <rdg wit="#Ϛ">ore</rdg>
                                    </app>,</l>
                                
                                <l n="32" xml:id="l1.32">sitis <app>
                                        <lem wit="#N #A #F #P #V #Vo">et</lem>
                                        <rdg source="#Richards">ut</rdg>
                                    </app> in tuto semper amore pares. </l>
                            </lg>
                       
                            <lg type="couplet">
                                <l n="33" xml:id="l1.33">in me <app>
                                        <lem>nostra</lem>
                                        <rdg source="#Francius">dura</rdg>
                                        <rdg source="#Richards">uestra</rdg>
                                    </app> Venus noctes exercet amaras,</l>
                                <l n="34" xml:id="l1.34">et nullo vacuus tempore defit Amor.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="35" xml:id="l1.35">hoc, moneo, vitate malum: sua quemque moretur</l>
                                <l n="36" xml:id="l1.36">cura, neque assueto mutet amore <app>
                                        <lem>locum</lem>
                                        <rdg source="#Otto #Housman">torum</rdg>
                                    </app>.</l>
                            </lg>
                        
                            <lg type="couplet">
                                <l n="37" xml:id="l1.37">quod si quis monitis tardas adverterit aures,</l>
                                <l n="38" xml:id="l1.38">heu referet quanto verba dolore mea!</l>
                            </lg>
                        </lg>
              
              </div>
          <div type="textpart" n="2" xml:id="poem2">

              <head>Poem 2</head>
